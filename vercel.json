{
  "version": 2,
  "builds": [
    {
      "src": "src/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/register",
      "dest": "src/server.js"
    },
    {
      "src": "/login",
      "dest": "src/server.js"
    },
    {
      "src": "/register",
      "dest": "src/server.js"
    },
    {
      "src": "/user_profile(.*)",
      "dest": "src/server.js"
    },
    {
      "src": "/users",
      "dest": "src/server.js"
    },
    {
      "src": "/group(.*)",
      "dest": "src/server.js"
    },
    {
      "src": "/register",
      "dest": "src/server.js"
    }
  ]
}
